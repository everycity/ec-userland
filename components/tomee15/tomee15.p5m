# Transforms
<include ec-drop-common>
<transform file path=ec/etc/tomee15/.* -> default group tomcat>
<transform file dir link hardlink path=ec/etc/tomee15.* -> default preserve true>
<transform file path=ec/etc/tomee15.* -> default mode 0644>
<transform file dir path=ec/etc/tomee15.* -> default owner tomcat>
<transform file dir path=ec/etc/tomee15.* -> default group tomcat>
<transform dir path=ec/var/tomee15/.* -> default owner tomcat>
<transform dir path=ec/var/tomee15/.* -> default group tomcat>
<transform dir path=ec/var/log/tomee15.* -> default owner tomcat>
<transform dir path=ec/var/log/tomee15.* -> default group tomcat>
# Drop files from the admin package
<transform file path=.*/lib/tomee15/webapps/host-manager.*$ -> drop>
<transform file path=.*/lib/tomee15/webapps/manager.*$ -> drop>

# Metadata
set name=pkg.fmri value=pkg:/web/java-servlet/tomee15@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)
set name=pkg.summary value="Tomcat J2EE Servlet/JSP Container"
set name=info.upstream_url value="http://tomee.apache.org/"
set name=info.source_url value=$(COMPONENT_ARCHIVE_URL)
license LICENSE license="Apache v2.0"

# Dependencies
depend fmri=developer/java/openjdk7 type=require
depend fmri=web/tomcat-common type=require

# SMF Entries
file files/http-tomee15 path=$(SVCMETHOD)/http-tomee15 mode=0555
file files/http-tomee15.xml path=$(SVCMANIFEST)/site/http-tomee15.xml \
    restart_fmri=svc:/system/manifest-import:default

# Helpful Links
link path=$(USRDIR)/var/tomee15/conf target=../../etc/tomee15
link path=$(USRDIR)/var/tomee15/logs target=../../var/log/tomee15
link path=$(USRDIR)/var/tomee15/webapps/ROOT \
    target=../../../lib/tomee15/webapps/ROOT preserve=true

